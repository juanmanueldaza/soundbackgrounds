{
  "name": "soundbackgrounds",
  "version": "0.0.0",
  "description": "Framework agnostic soundbackgrounds reactive to sound input. Users can load their own cartridges for visualizations.",
  "main": "dist/soundbackgrounds.umd.js",
  "module": "dist/soundbackgrounds.es.js",
  "exports": {
    ".": {
      "import": "./dist/soundbackgrounds.es.js",
      "require": "./dist/soundbackgrounds.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "deploy": "gh-pages -d dist",
    "audit": "npm audit",
    "profile:memory": "node --inspect scripts/profile-memory.js",
    "profile:cpu": "node --prof scripts/profile-cpu.js",
    "test:e2e": "playwright test",
    "test:perf": "node scripts/performance-test.js",
    "docs:generate": "typedoc src/",
    "docs:serve": "docsify serve docs"
  },
  "dependencies": {
    "hello-world-npm": "^1.1.1",
    "p5": "^1.11.3"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "jsdom": "^26.0.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "sound",
    "visualization",
    "p5.js",
    "reactive",
    "performance-optimized",
    "memory-safe",
    "real-time-audio"
  ],
  "author": "Juan Manuel Daza <juanmanueldaza@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juanmanueldaza/soundbackgrounds.git"
  },
  "bugs": {
    "url": "https://github.com/juanmanueldaza/soundbackgrounds/issues"
  },
  "homepage": "https://soundbackgrounds.ultravietnamita.com.ar"
}
